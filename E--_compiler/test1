
    event X(int a1 , int a2 , int a3);	                 //pass parameter, int calculation
    event Y(double a4, double a5 , double a6);      //float calculation
    event W();                                                                   //if statement
    event Z();                                                                   //while statement
    event S();                                                                   //optimization

    double b1, b2, b3, b4, b5 ;
    int c1, c2, c3, c4, c5;

    int t1=1;
    int t2=2;
    int t3=3;

    int f1=1;
    int f2=t1+t2;
    int f3=t1+t2*t3;

    double t4=0.0;
    double t5=2.5;
    double t6=7.5;

    double f4=t4*t5;
    double f5=t6/t5;
    double f6=t5+t6;


    
    // f1=1   f2=3   f3=7   c1=56   c2=-9
    X(f1, f2, f3) -> {
    	print "f1=";
    	print f1;
    	print "\n";
    	print "f2=";
    	print f2;
    	print "\n";
    	print "f3=";
    	print f3;
    	print "\n";

    	c1=(  (f1+f2) - (f2-f3)  )   *   (  (f3/f1) + (f2%f1)  );

         c2=(  (f1+f2) - (10-f2)  )   *   (  (f3/2) + (f2%3)  );

    	print "c1=";
    	print c1;
     	print "\n";   	
         print "c2=";
         print c2;
         print "\n";          
    };


    //  f4=0.0   f5=3.0   f6=10.0   b1=371293.0    b2=-35.0
    Y(f4, f5, f6) -> {
    	print "f4=";
    	print f4;
    	print "\n";
    	print "f5=";
    	print f5;
    	print "\n";
    	print "f6=";
    	print f6;
    	print "\n";

    	b1=(f5+f6) * (f5+f6) * (f5+f6) * (f5+f6) * (f5+f6);

         b2=(f6-5.0) * (f5-10.0) / (1.0+f4)

    	print "b1=";
    	print b1;
     	print "\n";   
         print "b2=";
         print b2;
         print "\n";  	
    };



    //  here
    W() -> {
        c1 = 5;
        if (c1>=5) {
                c2 = 4;
                c3 = 3;
                if (c2+c3<c5){
                    print "not here";
                    print "\n";
                }
                else {
                    print "here";
                    print "\n";
                }
        }
        else {
                print "not here";
                print "\n";
        }
    };



    // 0 0 0 0 0 0
    Z() -> {
        c1 = 5;
        while (c1>=0){
            c1=c1-1;
            c2 = -3;
            while (c2<0){
                c2 = c2+1;
            }
            print c2;
            print " ";
        }
        print "\n";
    }



    S() -> {
        c4 = 5;
        c4=c4+0;
        c4=0+c4;
        c4=c4-0;
        c4=c4*1;
        c4=1*c4;
        c4=c4/1;
        c4=0*c4;
        c4=c4*0;
        print "c4=";
        print c4;
        print "\n";

        c4=5;
        c4=c4-c4;
        c4=c4*2;
        c4=2*c4;
        c4=1+3;
        print "c4=";
        print c4;
        print "\n";

        b4=1.0+3;
        b4=1+3.0;
        b4=1.0+3.0;
        print "b4=";
        print b4;
        print "\n";

        c5=0;
        while (c5<100000000){
            c4 = 5;
            c4=c4+0;
            c4=0+c4;
            c4=c4-0;
            c4=c4*1;
            c4=1*c4;
            c4=c4/1;
            c4=0*c4;
            c4=c4*0;

            c4=5;
            c4=c4-c4;
            c4=c4*2;
            c4=2*c4;

            c4=1+3;
            b4=1.0+3;
            b4=1+3.0;
            b4=1.0+3.0;

            c5=c5+1;
            c4=(f1+f2)*(f2-f3)/(f3+f1+f1);
        }
        print "c4=";
        print c4;
        print "\n";
    }

    
   



